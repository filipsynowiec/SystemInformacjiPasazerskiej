            <Insets top="5.0" />
         </HBox.margin>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </VBox>
      <ListView fx:id="trasyListView" maxHeight="0">
         <VBox.margin>
            <Insets bottom="50.0" />
         </VBox.margin></ListView>


      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
   </VBox>

</ScrollPane>
