               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
         </HBox>
      </VBox>

      <VBox>
         <Label text="Usuń pociąg">
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <font>
               <Font name="System Bold" size="15.0" />
            </font>
         </Label>
         <HBox spacing="10.0">
            <ComboBox fx:id="deletePociagBox" editable="true" />
            <Button fx:id="deletePociagButton" text="Usuń" />
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
         </HBox>
      </VBox>

      <VBox>
         <Label text="Usuń wagon">
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <font>
               <Font name="System Bold" size="15.0" />
            </font>
         </Label>
         <HBox spacing="10.0">
            <Label text="Według modelu: ">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="deleteWagonBox" editable="true" />
            <Button fx:id="deleteWagonButton" text="Znajdź" />
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
         </HBox>
         <ListView fx:id="wagonList" maxHeight="0" />
      </VBox>

      <VBox>
         <Label text="Usuń skład">
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <font>
               <Font name="System Bold" size="15.0" />
            </font>
         </Label>
         <HBox spacing="10.0">
            <Label text="Liczba różnych modeli: ">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="deleteSkladField" editable="true" />
            <VBox.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
         </HBox>
         <HBox spacing="10.0">
            <Label text="Nazwa modelu występującego: ">
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
            </Label>
            <ComboBox fx:id="deleteSkladBox" editable="true" />
            <VBox.margin>
