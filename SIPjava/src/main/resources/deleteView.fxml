            <Font name="System Bold" size="15.0" />
         </font>
      </Label>

         <HBox spacing="10.0">
         <Label text="Z: ">
            <HBox.margin>
               <Insets top="5.0" />
            </HBox.margin>
         </Label>
         <ComboBox fx:id="deleteTrasaFromBox" editable="true" />
