                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>

            <HBox spacing="10.0">
               <Label text="Według trasy: ">
                  <HBox.margin>
                     <Insets top="5.0" />
                  </HBox.margin>
               </Label>

               <Label text="Z: ">
                  <HBox.margin>
                     <Insets top="5.0" />
                  </HBox.margin>
               </Label>
               <ComboBox fx:id="deleteRideFromBox" editable="true" />
               <Label text="Do: ">
                  <HBox.margin>
                     <Insets top="5.0" />
                  </HBox.margin>
               </Label>
               <ComboBox fx:id="deleteRideToBox" editable="true" />
               <ChoiceBox fx:id="dayRideBox"/>
               <Button fx:id="deleteRideButton" text="Znajdź" />
               <HBox.margin>
                  <Insets top="5.0" />
               </HBox.margin>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </HBox>
         <ListView fx:id="rideList" maxHeight="0" />
      </VBox>

      <VBox>

      <Label text="Usuń trasę" >
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
