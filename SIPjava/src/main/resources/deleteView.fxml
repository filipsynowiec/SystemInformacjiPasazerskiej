<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="systeminformacjipasazerskiej.controller.DeleteController">

   <VBox>
      <Label text="Usuń stację" />
      <HBox spacing="10.0">
         <ComboBox fx:id="deleteStationBox" editable="true" />
         <Button fx:id="deleteStationButton" text="Usuń" />
      </HBox>
      <Label text="Usuń wagon" />
      <HBox spacing="10.0">
         <ComboBox fx:id="deleteTruckBox" editable="true" />
         <Button fx:id="deleteTruckButton" text="Usuń" />
      </HBox>
      <Label text="Usuń pociąg" />
      <HBox spacing="10.0">
         <ComboBox fx:id="deleteTrainBox" editable="true" />
         <Button fx:id="deleteTrainButton" text="Usuń" />
      </HBox>
      <Label text="Usuń kurs" />
      <HBox spacing="10.0">
         <ComboBox fx:id="deleteRideFromBox" editable="true" />
         <ComboBox fx:id="deleteRideToBox" editable="true" />
         <Button fx:id="deleteRideButton" text="Znajdź" />
      </HBox>
   </VBox>

</ScrollPane>
